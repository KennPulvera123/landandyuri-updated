{
  "name": "lance-yuri-kids-spot",
  "version": "1.0.0",
  "description": "MERN stack website for Lance and Yuri Kids Spot Naga City",
  "main": "index.js",
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd api && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd api && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "start": "cd api && npm start"
  },
  "keywords": ["mern", "pediatric", "therapy", "booking"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
} 